alias ls='ls --color=auto'
alias mutt='neomutt'
alias rsync='rsync --progress'
alias virsh='sudo virsh'
alias vls='virsh list --all'
alias vstart='virsh start'
alias vstop='virsh shutdown'
alias virt-viewer='sudo virt-viewer'
alias vview='virt-viewer --connect qemu:///session --wait'
alias tails='sudo qemu-system-x86_64 -enable-kvm -cpu host -smp 4 -m 8192 -hda /dev/sdb'
